<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">


    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">


        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:orientation="horizontal"
            android:theme="@style/ToolbarStyle"
            android:background="@drawable/gradiant_back"
            app:popupTheme="@style/AppTheme.PopupOverlay">

            <ImageView
                android:id="@+id/iv_actionbar_back"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="start"
                android:paddingEnd="@dimen/dimen_16dp"
                app:srcCompat="@drawable/back_icon"
                android:tint="@color/black"
                tools:ignore="ContentDescription,RtlSymmetry" />

            <TextView
                android:id="@+id/tv_toolbar_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:fontFamily="@font/montserrat_regular"
                android:padding="@dimen/_5sdp"
                android:text="@string/text_support"
                android:textColor="@color/black"
                android:textSize="@dimen/dimen_18sp"
                tools:ignore="HardcodedText" />

        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:padding="12dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/app_bar">


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_lorem_ipsum"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/lorem_ipsum"
            android:textColor="@color/color_text_9F9F9F"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginTop="30dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/lorem_ipsum"
            android:textColor="@color/color_text_9F9F9F"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_lorem_ipsum"
            app:srcCompat="@drawable/yasmart_support_mail"

            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/text_email"
            android:textColor="@color/color_text_9F9F9F"
            android:textSize="13dp"
            app:fontFamily="@font/montserrat_medium"
            app:layout_constraintBottom_toBottomOf="@id/iv_email"
            app:layout_constraintLeft_toRightOf="@id/iv_email"
            app:layout_constraintTop_toTopOf="@id/iv_email" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_yasmart_support_gmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/montserrat_bold"
            android:text="@string/yas_mart_support_gmail"
            android:textColor="@color/black"
            android:textSize="17dp"
            app:fontFamily="@font/montserrat_medium"
            app:layout_constraintLeft_toLeftOf="@id/tv_email"
            app:layout_constraintTop_toBottomOf="@id/tv_email" />


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_yas_mart_support"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/lorem_ipsum"
            android:textColor="@color/color_text_9F9F9F"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_yasmart_support_gmail"
            app:srcCompat="@drawable/yasmart_support_icon"

            />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_want_call_with_us"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/want_call_with_us"
            android:textColor="@color/color_text_9F9F9F"
            android:textSize="13dp"
            app:fontFamily="@font/montserrat_medium"
            app:layout_constraintBottom_toBottomOf="@id/iv_yas_mart_support"
            app:layout_constraintLeft_toRightOf="@id/iv_yas_mart_support"
            app:layout_constraintTop_toTopOf="@id/iv_yas_mart_support" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_1800_3988_3988"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/montserrat_bold"
            android:text="@string/text_1800_3988_3988"
            android:textColor="@color/black"
            android:textSize="17dp"
            app:fontFamily="@font/montserrat_medium"
            app:layout_constraintLeft_toLeftOf="@id/tv_want_call_with_us"
            app:layout_constraintTop_toBottomOf="@id/tv_want_call_with_us"

            />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_taxi_support_call"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/text_email"
            android:textColor="@color/color_text_9F9F9F"
            android:textSize="13dp"
            app:srcCompat="@drawable/taxi_support_call"
            app:fontFamily="@font/montserrat_medium"
            app:layout_constraintBottom_toBottomOf="@id/tv_1800_3988_3988"
            app:layout_constraintTop_toTopOf="@id/tv_1800_3988_3988"
            app:layout_constraintRight_toRightOf="parent"
            />


        <androidx.appcompat.widget.AppCompatTextView

            android:id="@+id/btn_send_us_message"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:padding="15dp"
            android:drawableEnd="@drawable/setting_right_arrow_white"
            android:fontFamily="@font/montserrat_bold"
            android:text="@string/text_send_us_message_btn"
            android:textColor="@color/white"
            android:textSize="14dp"

            android:background="@drawable/round_corner_28_background"
            app:fontFamily="@font/montserrat_medium"
            app:layout_constraintLeft_toLeftOf="@id/tv_1800_3988_3988"
            app:layout_constraintTop_toBottomOf="@id/tv_1800_3988_3988"

            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>